<!--=============== Core ====================-->
  <RecipeDef>
    <defName>OE_Reset_OES_LegionSpear_A</defName>
    <label>Reset Legion Spear</label>
    <description>Reset a Legion Spear.\n\nWarning! Item Quality and Persona traits will be changed.</description>
    <jobString>Resetting a Legion Spear.</jobString>
	<workAmount>500</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	<unfinishedThingDef>OE_UnfinishedGear</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>OES_LegionSpear_A</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Syntherius</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>OES_LegionSpear_A</li>
		<li>OE_Syntherius</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <OES_LegionSpear_A>1</OES_LegionSpear_A>
    </products>
	<recipeUsers>
      <li>OE_PersonaTable</li>
    </recipeUsers>
	<skillRequirements>
	  <Crafting>12</Crafting>
	  <Intellectual>12</Intellectual>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

