<Defs>
<!--=============== OTC - Hammer ====================-->
  <RecipeDef>
    <defName>OE_Reset_OTC_Hammer_P_Acid</defName>
    <label>Reset Persona Nemesis Prototype(Acid)</label>
    <description>Reset an Persona Nemesis Prototype(Acid).\n\nWarning! Item Quality and Persona traits will be changed.</description>
    <jobString>Resetting a Persona Nemesis Prototype(Acid).</jobString>
	<workAmount>500</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	<unfinishedThingDef>OE_UnfinishedGear</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>OTC_Hammer_P_Acid</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Syntherius</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>OTC_Hammer_P_Acid</li>
		<li>OE_Syntherius</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <OTC_Hammer_P_Acid>1</OTC_Hammer_P_Acid>
    </products>
	<recipeUsers>
      <li>OE_PersonaTable</li>
    </recipeUsers>
	<skillRequirements>
	  <Crafting>12</Crafting>
	  <Intellectual>12</Intellectual>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
	<researchPrerequisite>OTCGearMod</researchPrerequisite>
  </RecipeDef>

