<!--=============== Namer - Obsidia Dialects ====================-->
<!--=============== Standard ====================-->
  <RulePackDef>
    <defName>OE_Namer_Obsidia_Dialect_Standard</defName>
	<rulePack>
	  <rulesStrings>
	    <li>r_name(p=0.2)->[anyPrefix][wholeName]</li>
		<li>r_name(p=0.2)->[anyPrefix]-[wholeName]</li>
		<li>r_name(p=1)->[wholeName]</li>
		<!---->
		<li>anyPrefix->zer</li>
		<li>anyPrefix->magna</li>
		<li>anyPrefix->vox</li>
		<li>anyPrefix->imper</li>
		<li>anyPrefix->strato</li>
		<li>anyPrefix->ordi</li>
		<li>anyPrefix->domina</li>
		<!---->
		<li>wholeName->obsilog</li>
		<li>wholeName->domingloss</li>
		<li>wholeName->corevox</li>
		<li>wholeName->lexidra</li>
		<li>wholeName->formantra</li>
		<li>wholeName->codentum</li>
		<li>wholeName->dictaryn</li>
		<li>wholeName->codivox</li>
		<li>wholeName->imperix</li>
		<li>wholeName->reglexia</li>
	  </rulesStrings>
	</rulePack>
  </RulePackDef>

