<!--=============== Hemogen Animal ====================-->
  <ThingDef ParentName="OEBaseLiquidSlime">
    <defName>OE_Slime_HemogenAnimal</defName>
    <label>animal hemogen slime</label>
    <description>Slimes - an amalgam life-form that was developed as an alternative source of necessary materials for Obsidia Expansion.\n\nAnimal hemogen slime production type, produces 4 Animal hemogen every 2 days.</description>
	<descriptionHyperlinks>
      <ThingDef>OE_SlimeHeart_HemogenAnimal</ThingDef>
	  <ThingDef>VRE_HemogenPack_Animal</ThingDef>
    </descriptionHyperlinks>
    <statBases>
      <MarketValue>350</MarketValue>
    </statBases>
	<race>
      <predator>true</predator>
      <foodType>CarnivoreAnimalStrict</foodType>
    </race>
	<butcherProducts>
      <VRE_HemogenPack_Animal>5</VRE_HemogenPack_Animal>
    </butcherProducts>
	<comps>
      <li Class="CompProperties_Milkable">
        <milkDef>VRE_HemogenPack_Animal</milkDef>
        <milkIntervalDays>2</milkIntervalDays>
        <milkAmount>4</milkAmount>
        <milkFemaleOnly>false</milkFemaleOnly>
      </li>
    </comps>
  </ThingDef>

  <PawnKindDef ParentName="AnimalKindBase">
    <defName>OE_Slime_HemogenAnimal</defName>
    <label>animal hemogen slime</label>
    <labelPlural>animal hemogen slimes</labelPlural>
    <race>OE_Slime_HemogenAnimal</race>
	<ecoSystemWeight>0.15</ecoSystemWeight>
	<canArriveManhunter>false</canArriveManhunter>
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/Slime/HemogenAnimal/Slime</texPath>
          <drawSize>0.35</drawSize>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Slime/HemogenAnimal/Dessicated_Slime</texPath>
          <drawSize>0.35</drawSize>
        </dessicatedBodyGraphicData>
      </li>
	  <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/Slime/HemogenAnimal/Slime</texPath>
          <drawSize>0.65</drawSize>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Slime/HemogenAnimal/Dessicated_Slime</texPath>
          <drawSize>0.65</drawSize>
        </dessicatedBodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/Slime/HemogenAnimal/Slime</texPath>
          <drawSize>1.0</drawSize>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Slime/HemogenAnimal/Dessicated_Slime</texPath>
          <drawSize>1.0</drawSize>
        </dessicatedBodyGraphicData>
      </li>
    </lifeStages>
	<combatPower>35</combatPower>
  </PawnKindDef>

  <ThingDef ParentName="OESlimeHeartBase">
    <defName>OE_SlimeHeart_HemogenAnimal</defName>
    <label>animal hemogen slime heart (Сonf.)</label>
    <description>A hyper-advanced slime core with configuration to grow up into Animal hemogen slime in 7 days. Animal hemogen slime produces 4 Animal hemogen every 2 days.\n\nWarning! Slime is formed untamed.</description>
	<descriptionHyperlinks>
      <ThingDef>OE_Slime_HemogenAnimal</ThingDef>
    </descriptionHyperlinks>
    <graphicData>
      <texPath>Things/Item/Resource/Special/SlimeHeart/OE_SlimeHeart_HemogenAnimal</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>250</MarketValue>
	  <WorkToMake>20000</WorkToMake>
    </statBases>
	<costList>
      <OE_SlimeHeart>1</OE_SlimeHeart>
	  <VRE_HemogenPack_Animal>25</VRE_HemogenPack_Animal>
    </costList>
	<comps>
	  <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>7</hatcherDaystoHatch>
        <hatcherPawn>OE_Slime_HemogenAnimal</hatcherPawn>
      </li>
    </comps>
  </ThingDef>

