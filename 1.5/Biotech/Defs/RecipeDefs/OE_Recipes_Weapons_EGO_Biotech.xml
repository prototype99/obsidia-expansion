<?xml version="1.0" encoding="utf-8" ?>

<Defs>
<!--=============== EGO - GrenadeLauncher ====================-->
  <RecipeDef>
    <defName>OE_Gestate_EGO_GrenadeLauncher</defName>
    <label>gestate Sporothrix</label>
    <description>Gestate an amalgam Grenade Launcher - Sporothrix.</description>
    <jobString>Gestating a Sporothrix.</jobString>
	<workAmount>7000</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	<unfinishedThingDef>OE_UnfinishedAmalgama</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>MeatRaw</li>
          </categories>
        </filter>
        <count>150</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Platinum</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Syntherius</li>
          </thingDefs>
        </filter>
        <count>12</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Wastepack</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_EGOSeed</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
	  <categories>
        <li>MeatRaw</li>
      </categories>  
	  <thingDefs>
		<li>Wastepack</li>
		<li>OE_Platinum</li>
		<li>OE_Syntherius</li>
		<li>OE_EGOSeed</li>
      </thingDefs>
	  <disallowedThingDefs>
        <li>OE_Pearlapple_Fruit</li>
		<li>OE_Red_TionantJelly</li>
        <li>InsectJelly</li>
      </disallowedThingDefs>
    </fixedIngredientFilter>
    <products>
      <EGO_GrenadeLauncher>1</EGO_GrenadeLauncher>
    </products>
	<recipeUsers>
      <li>OE_AmalgamVat</li>
    </recipeUsers>
	<skillRequirements>
	  <Crafting>8</Crafting>
	  <Intellectual>8</Intellectual>
	  <Medicine>8</Medicine>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
	<researchPrerequisites>
	  <li>OEGearCross</li>
	  <li>OEBioengineeringAdv</li>
	</researchPrerequisites>
  </RecipeDef>

