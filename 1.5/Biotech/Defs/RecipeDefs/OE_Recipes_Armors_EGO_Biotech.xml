          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_EGOSeed</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
	<fixedIngredientFilter>
	  <thingDefs>
		<li>EGO_Armor_Shell_Mechanitor</li>
		<li>OE_EGOSeed</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <EGO_Armor_Shell_Mechanitor>1</EGO_Armor_Shell_Mechanitor>
    </products>
	<recipeUsers>
      <li>OE_AmalgamVat</li>
    </recipeUsers>
	<skillRequirements>
	  <Crafting>6</Crafting>
	  <Intellectual>6</Intellectual>
	  <Medicine>6</Medicine>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
	<researchPrerequisites>
	  <li>OEGearCross</li>
	  <li>OEBioengineering</li>
	</researchPrerequisites>
  </RecipeDef>

