<Defs>
<!--=============== Maid ====================-->
  <RecipeDef ParentName="MediumMechanoidRecipe">
    <defName>OE_Mech_Maid</defName>
    <label>gestate maid</label>
    <description>Gestate a maid mechanoid.</description>
    <researchPrerequisite>OEMechtech</researchPrerequisite>
	<workAmount>3600</workAmount>
	<formingTicks>240000</formingTicks>
    <ingredients Inherit="False">
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Platinum</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Mythril</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>SubcoreHigh</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <OE_Mech_Maid>1</OE_Mech_Maid>
    </products>
    <descriptionHyperlinks>
      <ThingDef>OE_Mech_Maid</ThingDef>
    </descriptionHyperlinks>
  </RecipeDef>

