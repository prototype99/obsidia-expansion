<!--=============== Acid ====================-->
  <HediffDef ParentName="OEImplantBodyParts">
    <defName>OE_ImplantArm_Sting_Acid</defName>
    <label>arm implant - Sting(Acid)</label>
    <labelNoun>an arm implant - Sting</labelNoun>
    <description>An installed arm implant - Sting.</description>
    <descriptionHyperlinks><ThingDef>OE_ImplantArm_Sting_Acid</ThingDef></descriptionHyperlinks>
    <spawnThingOnRemoved>OE_ImplantArm_Sting_Acid</spawnThingOnRemoved>
	<addedPartProps>
      <solid>true</solid>
    </addedPartProps>
	<stages>
	  <li>
	    <hungerRateFactorOffset>0.1</hungerRateFactorOffset>
	  </li>
	</stages>
	<comps>
      <li Class="HediffCompProperties_VerbGiver">
        <tools>
          <li>
            <label>sting</label>
            <capacities>
              <li>Poke</li>
            </capacities>
            <power>5</power>
            <armorPenetration>0.35</armorPenetration>
            <cooldownTime>1.75</cooldownTime>
            <alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
            <soundMeleeHit>MeleeHit_BionicPunch</soundMeleeHit>
            <soundMeleeMiss>MeleeMiss_BionicPunch</soundMeleeMiss>
			<extraMeleeDamages>
              <li>
                <def>OTCAcid</def>
                <amount>15</amount>
              </li>
            </extraMeleeDamages>
          </li>
        </tools>
      </li>
    </comps>
	<tags><li>ArmImplant</li></tags>
  </HediffDef>

  <ThingDef ParentName="OEBodyPartsM">
    <defName>OE_ImplantArm_Sting_Acid</defName>
    <label>arm implant - Sting(Acid)</label>
    <description>Obsidia arm implant combat-oriented mechanical sting installing into arm. Charged with Acid damage.</description>
