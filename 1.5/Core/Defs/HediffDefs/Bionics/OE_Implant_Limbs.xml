	<graphicData>
      <texPath>Things/Item/Health/Implant/OTC/OE_Arm</texPath>
	  <drawSize>0.80</drawSize>
    </graphicData>
	<techHediffsTags>
      <li>OTCBionics</li>
    </techHediffsTags>
    <costList>
      <OE_DragonianWoodLog>20</OE_DragonianWoodLog>
	  <OE_Obsidian>10</OE_Obsidian>
	  <OE_Platinum>10</OE_Platinum>
      <OE_Syntherius>25</OE_Syntherius>
    </costList>
	<butcherProducts>
	  <OE_DragonianWoodLog>10</OE_DragonianWoodLog>
	  <OE_Obsidian>10</OE_Obsidian>
	  <OE_Platinum>5</OE_Platinum>
      <OE_Syntherius>10</OE_Syntherius>
	</butcherProducts>
    <statBases>
      <Mass>5.0</Mass>
    </statBases>
	<recipeMaker>
      <researchPrerequisite>OEImplants</researchPrerequisite>
    </recipeMaker>
  </ThingDef>

  <RecipeDef ParentName="OESurgeryInstalImplantBase">
    <defName>OE_InstallImplantArm_Sting_Acid</defName>
    <label>install arm implant - Sting(Acid)</label>
    <description>Install an arm implant - Sting.</description>
    <descriptionHyperlinks>
      <ThingDef>OE_ImplantArm_Sting_Acid</ThingDef>
      <HediffDef>OE_ImplantArm_Sting_Acid</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing arm implant - Sting.</jobString>
	<uiIconThing>OE_ImplantArm_Sting_Acid</uiIconThing>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>OE_ImplantArm_Sting_Acid</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>OE_ImplantArm_Sting_Acid</li>
      </thingDefs>
    </fixedIngredientFilter>
