<Defs>
<!--=============== Stomach ====================-->
<!--=============== Implant - Traveler ====================-->
  <HediffDef ParentName="OEImplantBodyParts">
    <defName>OE_ImplantStomach_Traveler</defName>
    <label>stomach implant - Traveler</label>
    <labelNoun>a stomach implant - Traveler</labelNoun>
    <description>An installed stomach implant - Traveler.</description>
    <descriptionHyperlinks><ThingDef>OE_ImplantStomach_Traveler</ThingDef></descriptionHyperlinks>
    <spawnThingOnRemoved>OE_ImplantStomach_Traveler</spawnThingOnRemoved>
	<stages>
	  <li>
	    <statOffsets>
		  <ForagedNutritionPerDay>10</ForagedNutritionPerDay>
	    </statOffsets>
	  </li>
	</stages>
  </HediffDef>

  <ThingDef ParentName="OEBodyPartsM">
    <defName>OE_ImplantStomach_Traveler</defName>
    <label>stomach implant - Traveler</label>
    <description>Obsidia traveler stomach implant was designed to additional nutrition while traveling by caravan.</description>
    <descriptionHyperlinks><RecipeDef>OE_InstallImplantStomach_Traveler</RecipeDef></descriptionHyperlinks>
	<graphicData>
      <texPath>Things/Item/Health/Implant/ODC/OE_Stomach</texPath>
	  <drawSize>0.80</drawSize>
    </graphicData>
	<techHediffsTags>
      <li>ODCBionics</li>
    </techHediffsTags>	
    <costList>
      <OE_PlatinumSynthweave>20</OE_PlatinumSynthweave>
	  <OE_Obsidian>10</OE_Obsidian>
	  <OE_Mythril>10</OE_Mythril>
      <OE_Syntherius>25</OE_Syntherius>
    </costList>
	<butcherProducts>
	  <OE_PlatinumSynthweave>10</OE_PlatinumSynthweave>
	  <OE_Obsidian>10</OE_Obsidian>
	  <OE_Mythril>5</OE_Mythril>
      <OE_Syntherius>10</OE_Syntherius>
	</butcherProducts>
    <statBases>
      <Mass>2.5</Mass>
    </statBases>
	<recipeMaker>
      <researchPrerequisite>OEImplants</researchPrerequisite>
    </recipeMaker>
  </ThingDef>

  <RecipeDef ParentName="OESurgeryInstalImplantBase">
    <defName>OE_InstallImplantStomach_Traveler</defName>
    <label>install stomach implant - Traveler</label>
    <description>Install a stomach implant - Traveler.</description>
    <descriptionHyperlinks>
      <ThingDef>OE_ImplantStomach_Traveler</ThingDef>
      <HediffDef>OE_ImplantStomach_Traveler</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing stomach implant - Traveler.</jobString>
	<uiIconThing>OE_ImplantStomach_Traveler</uiIconThing>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>OE_ImplantStomach_Traveler</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>OE_ImplantStomach_Traveler</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Stomach</li>
    </appliedOnFixedBodyParts>
    <addsHediff>OE_ImplantStomach_Traveler</addsHediff>
  </RecipeDef>

  <RecipeDef ParentName="SurgeryRemoveImplantBase">
    <defName>OE_RemoveImplantStomach_Traveler</defName>
    <label>remove stomach implant - Traveler</label>
    <description>Remove a stomach implant - Traveler.</description>
    <jobString>Removing stomach implant - Traveler.</jobString>
    <removesHediff>OE_ImplantStomach_Traveler</removesHediff>
	<uiIconThing>OE_ImplantStomach_Traveler</uiIconThing>
  </RecipeDef>

