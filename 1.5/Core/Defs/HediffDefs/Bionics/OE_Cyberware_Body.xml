<!--=============== Cyberware - Temperature ====================-->
  <HediffDef ParentName="OECyberwareLvlParts">
    <defName>OE_CyberwareTorso_Temperature</defName>
    <label>cyberware - Temperature protector</label>
    <labelNoun>a cyberware - Temperature protector</labelNoun>
    <description>An installed cyberware - Temperature protector.\n\nThis cyberware can be installed up to 5 times to increase its effect.</description>
    <descriptionHyperlinks><ThingDef>OE_CyberwareTorso_Temperature</ThingDef></descriptionHyperlinks>
    <spawnThingOnRemoved>OE_CyberwareTorso_Temperature</spawnThingOnRemoved>
    <minSeverity>0</minSeverity>
    <maxSeverity>5</maxSeverity>
	<stages>
      <li>
        <minSeverity>1</minSeverity>
        <statOffsets>
		  <Insulation_Cold>5</Insulation_Cold>
          <Insulation_Heat>5</Insulation_Heat>
        </statOffsets>
      </li>
      <li>
        <minSeverity>2</minSeverity>
        <statOffsets>
          <Insulation_Cold>10</Insulation_Cold>
          <Insulation_Heat>10</Insulation_Heat>
        </statOffsets>
      </li>
      <li>
        <minSeverity>3</minSeverity>
        <statOffsets>
          <Insulation_Cold>15</Insulation_Cold>
          <Insulation_Heat>15</Insulation_Heat>
        </statOffsets>
      </li>
      <li>
        <minSeverity>4</minSeverity>
        <statOffsets>
          <Insulation_Cold>20</Insulation_Cold>
          <Insulation_Heat>20</Insulation_Heat>
        </statOffsets>
      </li>
      <li>
        <minSeverity>5</minSeverity>
        <statOffsets>
          <Insulation_Cold>25</Insulation_Cold>
          <Insulation_Heat>25</Insulation_Heat>
        </statOffsets>
      </li>
    </stages>
  </HediffDef>

