<Defs>
<!--=============== OMG - Pistol ====================-->
  <ThingDef ParentName="OERangedBaseNQ">
    <defName>OMG_Pistol</defName>
    <label>Nanofire</label>
    <description>Highly modified Pistol used by Mercenaries. Specially modified weapon charged with Necrobane and EMP damages.\n\nThis modification deprives item's quality levels.</description>
    <graphicData>
      <texPath>Things/Item/Equipment/Weapons/OMG/Pistol</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
	<uiIconScale>1.2</uiIconScale>
	<soundInteract>Interact_ChargeRifle</soundInteract>
    <weaponClasses>
      <li>RangedLight</li>
    </weaponClasses>
	<weaponTags>
	  <li>OMGRanged</li>
    </weaponTags>
	<tradeTags>
      <li>OMGGear</li>
	  <li>OEGearSell</li>
    </tradeTags>
	<equippedStatOffsets>
	  <PainShockThreshold>-0.05</PainShockThreshold>
	  <PsychicSensitivity MayRequire="Ludeon.RimWorld.Royalty">0.25</PsychicSensitivity>
      <PsychicEntropyRecoveryRate MayRequire="Ludeon.RimWorld.Royalty">0.04</PsychicEntropyRecoveryRate>
	  <SocialIdeoSpreadFrequencyFactor MayRequire="Ludeon.RimWorld.Ideology">0.10</SocialIdeoSpreadFrequencyFactor>
	  <WorkSpeedGlobalOffsetMech MayRequire="Ludeon.RimWorld.Biotech">0.10</WorkSpeedGlobalOffsetMech>
	</equippedStatOffsets>
	<statBases>
      <MarketValue>4740</MarketValue>
	  <Mass>2.0</Mass>
	  <AccuracyTouch>1.10</AccuracyTouch>
      <AccuracyShort>1.00</AccuracyShort>
      <AccuracyMedium>0.75</AccuracyMedium>
      <AccuracyLong>0.50</AccuracyLong>
      <RangedWeapon_Cooldown>1.0</RangedWeapon_Cooldown>
    </statBases>
	<costList>
      <OE_Empyrium>40</OE_Empyrium>
	  <OE_Orichalcum>20</OE_Orichalcum>
      <OE_Syntherius>15</OE_Syntherius>
	  <OE_MercenaryKit>2</OE_MercenaryKit>
    </costList>
	<verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>Bullet_OMG_P</defaultProjectile>
        <warmupTime>0.75</warmupTime>
        <range>20.9</range>
        <burstShotCount>3</burstShotCount>
        <ticksBetweenBurstShots>7</ticksBetweenBurstShots>
		<soundCast>Shot_ChargeRifle</soundCast>
        <soundCastTail>GunTail_Light</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
    <tools>
      <li>
        <label>grip</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>10</power>
		<armorPenetration>0.45</armorPenetration>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>10</power>
		<armorPenetration>0.45</armorPenetration>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
	<recipeMaker>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Smith</effectWorking>
      <soundWorking>Recipe_Smith</soundWorking>
	  <recipeUsers>
        <li>OE_WeaponWorkbench</li>
      </recipeUsers>
	  <researchPrerequisite>OMGGear</researchPrerequisite>
	  <unfinishedThingDef>OE_UnfinishedRanged</unfinishedThingDef>
	  <skillRequirements>
        <Crafting>12</Crafting>
      </skillRequirements>
    </recipeMaker>
	<comps Inherit="False">
      <li Class="CompProperties_Forbiddable"/>
	  <li>
        <compClass>CompEquippable</compClass>
      </li>
	  <li Class="CompProperties_EquippableAbilityReloadable" MayRequire="Ludeon.RimWorld.Anomaly">
        <abilityDef>OMG_Ignite</abilityDef>
        <maxCharges>5</maxCharges>
        <soundReload>Standard_Reload</soundReload>
        <chargeNoun>ignite charge</chargeNoun>
        <ammoDef>OE_AmmoKit</ammoDef>
        <ammoCountPerCharge>2</ammoCountPerCharge>
        <baseReloadTicks>60</baseReloadTicks>
      </li>
    </comps>
	<thingCategories Inherit="False">
      <li>OE_Category_Weapons_OMG_Ranged</li>
    </thingCategories>
