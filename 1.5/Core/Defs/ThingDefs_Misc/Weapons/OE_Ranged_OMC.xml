  <ThingDef ParentName="OMCPistolBase">
    <defName>OMC_Pistol</defName>
    <label>Impaler</label>
    <description>Regular Auto Pistol used by OMC Soldiers. Rapid-fire and multiple-charge weapons with reduced damage.</description>
    <graphicData>
      <texPath>Things/Item/Equipment/Weapons/OMC/Pistol</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <weaponTags>
      <li>OMCLightGun</li>
    </weaponTags>
	<statBases>
      <MarketValue>440</MarketValue>
	  <AccuracyTouch>0.80</AccuracyTouch>
      <AccuracyShort>0.70</AccuracyShort>
      <AccuracyMedium>0.40</AccuracyMedium>
      <AccuracyLong>0.20</AccuracyLong>
      <RangedWeapon_Cooldown>1.25</RangedWeapon_Cooldown>
    </statBases>
	<verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>Bullet_OMC_P</defaultProjectile>
        <warmupTime>0.75</warmupTime>
        <range>20.9</range>
        <burstShotCount>4</burstShotCount>
        <ticksBetweenBurstShots>5</ticksBetweenBurstShots>
        <soundCast>Shot_Autopistol</soundCast>
        <soundCastTail>GunTail_Light</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
    <tools>
      <li>
        <label>grip</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>1.5</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
	<recipeMaker>
      <recipeUsers>
        <li>OE_WeaponWorkbench</li>
      </recipeUsers>
	  <researchPrerequisite>OMCWeapons</researchPrerequisite>	
    </recipeMaker>
    <costList>
      <OE_Obsidian>55</OE_Obsidian>
	  <OE_Mythril>40</OE_Mythril>
	  <OE_Mooncloth>2</OE_Mooncloth>
      <OE_Syntherius>5</OE_Syntherius>
    </costList>
  </ThingDef>

