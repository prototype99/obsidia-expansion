<!--=============== Obsidia Can Meal ====================-->
<!--=============== Normal ====================-->
  <RecipeDef ParentName="CookMealLavishBase">
    <defName>OE_CookCanMeal</defName>
    <label>cook obsidia can meal</label>
    <description>Cook an exquisite, high-quality meal from a combination of meat and plant ingredients. Jumy berries are delicately used as a spice to enhance the flavors. While much of the ingredients are meticulously trimmed away, this process ensures the creation of an unparalleled dining experience.</description>
    <jobString>Cooking obsidia can meal.</jobString>
    <ingredients>
      <li>
        <filter>
          <customSummary>protein</customSummary>
          <categories>
            <li>MeatRaw</li>
            <li>AnimalProductRaw</li>
          </categories>
        </filter>
        <count>0.5</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>PlantFoodRaw</li>
          </categories>
        </filter>
        <count>0.5</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Jumy_Berries</li>
          </thingDefs>
        </filter>
        <count>0.25</count>
      </li>
    </ingredients>
    <products>
      <OE_CanMeal>1</OE_CanMeal>
    </products>
	<recipeUsers>
      <li>FueledStove</li>
	  <li>ElectricStove</li>
	  <li MayRequire="VanillaExpanded.VFEProduction">VFE_TableStoveLarge</li>
    </recipeUsers>
	<researchPrerequisite>OECore</researchPrerequisite>
  </RecipeDef>

