<Defs>
<!--=============== Jumyline ====================-->
  <RecipeDef>
    <defName>OE_Cook_JumylinePack</defName>
    <label>cook jumyline candies x5</label>
    <description>Cook 5 jumyline candies from jumy berries and chocolate.</description>
    <jobString>Cooking 5 jumyline candies.</jobString>
	<workAmount>250</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <requiredGiverWorkType>Cooking</requiredGiverWorkType>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Chocolate</li>
          </thingDefs>
        </filter>
        <count>0.5</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Jumy_Berries</li>
          </thingDefs>
        </filter>
        <count>0.25</count>
      </li>
    </ingredients>
    <products>
      <OE_Jumyline_Candies>5</OE_Jumyline_Candies>
    </products>
	<recipeUsers>
      <li>FueledStove</li>
	  <li>ElectricStove</li>
	  <li MayRequire="VanillaExpanded.VFEProduction">VFE_TableStoveLarge</li>
    </recipeUsers>
	<skillRequirements>
	  <Cooking>6</Cooking>
	</skillRequirements>
    <workSkill>Cooking</workSkill>
	<researchPrerequisite>OECore</researchPrerequisite>
  </RecipeDef>

