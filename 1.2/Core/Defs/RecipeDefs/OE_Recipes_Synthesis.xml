<Defs>
<!--=============== Luciferium ====================-->
  <RecipeDef>
    <defName>OE_Synthesis_LuciferiumSingle</defName>
    <label>synthesis luciferium</label>
    <description>Synthesis luciferium from Neutroamine and Blue Tionant Jelly with Syntherius.</description>
    <jobString>Synthesis luciferium.</jobString>
	<workAmount>2500</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>OE_Syntherius</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Neutroamine</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Blue_TionantJelly</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>OE_Syntherius</li>
		<li>OE_Blue_TionantJelly</li>
		<li>Neutroamine</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Luciferium>1</Luciferium>
    </products>
	<recipeUsers>
      <li>OE_SynthesisPrinter</li>
    </recipeUsers>
	<skillRequirements>
      <Medicine>8</Medicine>
	  <Intellectual>10</Intellectual>
    </skillRequirements>
    <workSkill>Intellectual</workSkill>
	<researchPrerequisite>OESynthAdv</researchPrerequisite>
  </RecipeDef>

