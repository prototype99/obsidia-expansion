<Defs>
<!--=============== Textile ====================-->
<!--=============== Mooncloth ====================-->
  <ThingDef ParentName="ResourceBase">
    <defName>OE_Mooncloth</defName>
    <label>Mooncloth</label>
    <description>Fabric spun from microfibers extracted from Moonplants. It is tough, light, strong, luxurious and insulates well.</description>
    <graphicData>
      <texPath>Things/Item/Resource/Fabric/Textile</texPath>
	  <graphicClass>Graphic_StackCount</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
	  <color>(90,49,121)</color>
    </graphicData>
    <statBases>
      <MarketValue>7.5</MarketValue>
      <Mass>0.03</Mass>
	  <StuffPower_Armor_Sharp>1.75</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>0.45</StuffPower_Armor_Blunt>
      <StuffPower_Armor_Heat>2.5</StuffPower_Armor_Heat>
      <StuffPower_Insulation_Cold>30</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>30</StuffPower_Insulation_Heat>
	  <MaxHitPoints>150</MaxHitPoints>
      <Flammability>0.4</Flammability>
      <DeteriorationRate>1.0</DeteriorationRate>
    </statBases>
    <burnableByRecipe>true</burnableByRecipe>
    <healthAffectsPrice>false</healthAffectsPrice>
    <minRewardCount>30</minRewardCount>
    <stuffProps>
      <categories>
        <li>Fabric</li>
      </categories>
      <commonality>0.10</commonality>
      <color>(196,152,232)</color>
      <statFactors>
        <Flammability>0.4</Flammability>
        <MaxHitPoints>2.0</MaxHitPoints>
        <Beauty>4.0</Beauty>
      </statFactors>
    </stuffProps>
    <thingCategories>
      <li>Textiles</li>
    </thingCategories>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
  </ThingDef>

