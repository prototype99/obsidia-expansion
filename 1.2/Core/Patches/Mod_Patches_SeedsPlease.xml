<?xml version="1.0" encoding="utf-8" ?>

<Patch>

<Operation Class="PatchOperationFindMod">
	<mods>
	  <li>SeedsPlease</li>
	</mods>
	<match Class="PatchOperationSequence">
	  <operations>
	    <li Class="PatchOperationAdd">
		  <xpath>/Defs</xpath>
		  <value>
<!--=============== Tree Pearlapple ====================-->
  <SeedsPlease.SeedDef ParentName="SeedBase">
    <defName>Seed_OE_TreePearlapple</defName>
    <label>pearlapple tree seeds</label>
    <sources>
      <li>Plant_OE_TreePearlapple</li>
    </sources>
	<seed>
      <baseChance>0.70</baseChance>
      <extraChance>0.20</extraChance>
    </seed>
    <graphicData>
	  <texPath>Things/Item/Seeds/OE_Seed_Pearlapple</texPath>
	  <graphicClass>Graphic_Single</graphicClass>
	</graphicData>
  </SeedsPlease.SeedDef>

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_OE_TreePearlapple</defName>
    <label>Extract pearlapple tree seeds</label>
    <description>Extract seeds from pearlapple fruit.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>OE_Pearlapple_Fruit</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>OE_Pearlapple_Fruit</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_OE_TreePearlapple>2</Seed_OE_TreePearlapple>
    </products>
  </RecipeDef>

