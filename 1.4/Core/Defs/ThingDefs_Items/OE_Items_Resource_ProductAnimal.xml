<!--=============== Tionant Jelly ====================-->
<!--=============== Blue ====================-->
  <ThingDef ParentName="ResourceBase">
    <defName>OE_Blue_TionantJelly</defName>
    <label>blue tionant jelly</label>
    <description>Unique jelly produced by blue tionant. This medicinal jelly is used in medicine or in the synthesis of certain substances.</description>
	<descriptionHyperlinks>
	  <ThingDef>OE_Tionant_Blue</ThingDef>
    </descriptionHyperlinks>
    <graphicData>
      <texPath>Things/Item/Resource/ProductAnimal/JellyTionant/JellyBlue</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>0.85</drawSize>
    </graphicData>
    <uiIconForStackCount>1</uiIconForStackCount>
    <soundInteract>Standard_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <techLevel>Industrial</techLevel>
    <statBases>
      <MarketValue>125</MarketValue>
      <Mass>0.1</Mass>
	  <MaxHitPoints>50</MaxHitPoints>
      <Flammability>0.7</Flammability>
      <MedicalPotency>1.75</MedicalPotency>
      <MedicalQualityMax>1.75</MedicalQualityMax>
      <DeteriorationRate>1</DeteriorationRate>
    </statBases>
	<healthAffectsPrice>false</healthAffectsPrice>
	<thingClass>Medicine</thingClass>
    <stackLimit>25</stackLimit>
	<thingCategories>
      <li>Medicine</li>
    </thingCategories>
	<tradeTags>
      <li>OETionantGel</li>
    </tradeTags>
	<orderedTakeGroup>Medicine</orderedTakeGroup>
  </ThingDef>

<!--=============== Green ====================-->
  <ThingDef ParentName="ResourceBase">
    <defName>OE_Green_TionantJelly</defName>
    <label>green tionant jelly</label>
    <description>Unique jelly produced by the green tionant. This vegetable jelly is used as food or in cooking.</description>
	<descriptionHyperlinks>
	  <ThingDef>OE_Tionant_Green</ThingDef>
    </descriptionHyperlinks>
    <graphicData>
      <texPath>Things/Item/Resource/ProductAnimal/JellyTionant/JellyGreen</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>0.85</drawSize>
    </graphicData>
    <uiIconForStackCount>1</uiIconForStackCount>
    <techLevel>Industrial</techLevel>
    <statBases>
      <MarketValue>25</MarketValue>
      <Mass>0.1</Mass>
	  <MaxHitPoints>50</MaxHitPoints>
      <Flammability>0.7</Flammability>
	  <Nutrition>0.1</Nutrition>
      <FoodPoisonChanceFixedHuman>0.01</FoodPoisonChanceFixedHuman>
      <DeteriorationRate>1</DeteriorationRate>
    </statBases>
	<ingestible>
      <foodType>VegetableOrFruit, Fluid</foodType>
	  <preferability>RawTasty</preferability>
      <tasteThought>AteRawFood</tasteThought>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
	<healthAffectsPrice>false</healthAffectsPrice>
	<socialPropernessMatters>true</socialPropernessMatters>
	<thingCategories>
      <li>PlantFoodRaw</li>
    </thingCategories>
	<tradeTags>
      <li>OETionantGel</li>
    </tradeTags>
	<tickerType>Rare</tickerType>
	<comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>30</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>

