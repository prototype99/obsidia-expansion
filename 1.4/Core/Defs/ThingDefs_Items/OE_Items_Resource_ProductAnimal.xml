<Defs>
<!--=============== Raptors Egg ====================-->
  <ThingDef ParentName="EggUnfertBase">
    <defName>OE_EggRaptorUnfertilized</defName>
    <label>raptor egg (unfert.)</label>
    <description>An unfertilized raptor egg. It can be eaten raw, but it's much, much better cooked.</description>
	<descriptionHyperlinks>
	  <ThingDef>OE_Raptor</ThingDef>
    </descriptionHyperlinks>
    <graphicData>
      <texPath>Things/Item/Resource/ProductAnimal/EggRaptor</texPath>
	  <graphicClass>Graphic_StackCount</graphicClass>
	  <color>(55,54,52)</color>
    </graphicData>
    <statBases>
      <Mass>1.0</Mass>
      <MaxHitPoints>20</MaxHitPoints>
	  <MarketValue>20.0</MarketValue>
    </statBases>
  </ThingDef>

  <ThingDef ParentName="EggFertBase">
    <defName>OE_EggRaptorFertilized</defName>
    <label>raptor egg (fert.)</label>
    <description>Fertilized raptor egg. If all goes well, it should hatch into a baby raptor. It can be eaten raw, but it's much better cooked.</description>
	<descriptionHyperlinks>
	  <ThingDef>OE_Raptor</ThingDef>
    </descriptionHyperlinks>
    <graphicData>
      <texPath>Things/Item/Resource/ProductAnimal/EggRaptor</texPath>
	  <graphicClass>Graphic_StackCount</graphicClass>
	  <color>(98,93,89)</color>
    </graphicData>
    <statBases>
      <Mass>1.0</Mass>
      <MaxHitPoints>20</MaxHitPoints>
	  <MarketValue>20.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>10</hatcherDaystoHatch>
        <hatcherPawn>OE_Raptor</hatcherPawn>
      </li>
    </comps>
  </ThingDef>

