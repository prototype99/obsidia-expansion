          <def>Spine</def>
          <coverage>0.05</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Stomach</def>
          <coverage>0.05</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Heart</def>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Lung</def>
          <customLabel>left lung</customLabel>
          <coverage>0.04</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Lung</def>
