<!--=============== OES Eidolons ====================-->
  <PawnKindDef ParentName="OESPawn">
    <defName>OES_Eidolon_Melee</defName>
    <label>Obsidia Eidolon</label>
	<itemQuality>Excellent</itemQuality>
    <apparelMoney>25000~35000</apparelMoney>
	<apparelTags>
      <li>OESEidolon</li>
    </apparelTags>
	<apparelDisallowTags>
	  <li>OESEidolon_Barrier</li>
	  <li>OEModuleI</li>
    </apparelDisallowTags>
    <weaponMoney>15000~25000</weaponMoney>
	<weaponTags>
      <li>OESMelee</li>
    </weaponTags>
    <techHediffsMoney>25000~45000</techHediffsMoney>
	<techHediffsChance>1.00</techHediffsChance>
	<techHediffsMaxAmount>12</techHediffsMaxAmount>
	<techHediffsRequired>
      <li>OE_ImplantBrain_Interface</li>
    </techHediffsRequired>
	<techHediffsTags>
      <li>OEBionics_Advanced</li>
	  <li>OMCBionics</li>
	  <li>OTCBionics</li>
	  <li>OCCBionics</li>
	  <li MayRequire="Ludeon.RimWorld.Royalty">OECBionics</li>
	  <li>ODCBionics</li>
	  <li>CACBionics</li>
	  <li>OEBionics_Cyberware</li>
    </techHediffsTags>
	<combatPower>255</combatPower>
  </PawnKindDef>

