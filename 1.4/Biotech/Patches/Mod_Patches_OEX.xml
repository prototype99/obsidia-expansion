<?xml version="1.0" encoding="utf-8" ?>

<Patch>
<!--=============== Obsidia Expansion - Mothoids Xenotype ====================-->
<Operation Class="PatchOperationFindMod">
	<mods>
	  <li>Obsidia Expansion - Mothoids Xenotype</li>
	</mods>
	<match Class="PatchOperationSequence">
	  <operations>
	    <li Class="PatchOperationAdd">
		  <xpath>/Defs/FactionDef[defName="ObsidiaEmpire"]/xenotypeSet/xenotypeChances</xpath>
		  <value>
			<OE_Mothoid MayRequire="Ludeon.RimWorld.Biotech">0.02</OE_Mothoid>
			<OE_MothoidPrime MayRequire="Ludeon.RimWorld.Biotech">0.02</OE_MothoidPrime>
			<OE_MothoidAtlant MayRequire="Ludeon.RimWorld.Biotech">0.02</OE_MothoidAtlant>
		  </value>
	    </li>
		<li Class="PatchOperationAdd">
		  <xpath>/Defs/FactionDef[defName="ObsidiaSyndicateClan"]/xenotypeSet/xenotypeChances</xpath>
		  <value>
			<OE_MothoidPrime MayRequire="Ludeon.RimWorld.Biotech">0.02</OE_MothoidPrime>
			<OE_MothoidAtlant MayRequire="Ludeon.RimWorld.Biotech">0.02</OE_MothoidAtlant>
		  </value>
	    </li>
		<li Class="PatchOperationAdd">
		  <xpath>/Defs/FactionDef[defName="ObsidiaRenegadeClan"]/xenotypeSet/xenotypeChances</xpath>
		  <value>
			<OE_Mothoid MayRequire="Ludeon.RimWorld.Biotech">0.02</OE_Mothoid>
			<OE_MothoidAtlant MayRequire="Ludeon.RimWorld.Biotech">0.02</OE_MothoidAtlant>
		  </value>
	    </li>
		<li Class="PatchOperationAdd">
		  <xpath>/Defs/FactionDef[defName="ObsidiaPilgrimClan"]/xenotypeSet/xenotypeChances</xpath>
		  <value>
			<OE_Mothoid MayRequire="Ludeon.RimWorld.Biotech">0.02</OE_Mothoid>
			<OE_MothoidPrime MayRequire="Ludeon.RimWorld.Biotech">0.02</OE_MothoidPrime>
		  </value>
	    </li>
		<li Class="PatchOperationAdd">
		  <xpath>/Defs/FactionDef[defName="ObsidiaCrimsonArachnaCult"]/xenotypeSet/xenotypeChances</xpath>
		  <value>
			<OE_Mothoid MayRequire="Ludeon.RimWorld.Biotech">0.03</OE_Mothoid>
			<OE_MothoidPrime MayRequire="Ludeon.RimWorld.Biotech">0.03</OE_MothoidPrime>
			<OE_MothoidAtlant MayRequire="Ludeon.RimWorld.Biotech">0.03</OE_MothoidAtlant>
		  </value>
	    </li>
		<li Class="PatchOperationAdd">
		  <xpath>/Defs/FactionDef[defName="ObsidiaEmpireSins"]/xenotypeSet/xenotypeChances</xpath>
		  <value>
			<OE_Mothoid MayRequire="Ludeon.RimWorld.Biotech">0.03</OE_Mothoid>
			<OE_MothoidPrime MayRequire="Ludeon.RimWorld.Biotech">0.03</OE_MothoidPrime>
			<OE_MothoidAtlant MayRequire="Ludeon.RimWorld.Biotech">0.03</OE_MothoidAtlant>
		  </value>
	    </li>
	  </operations>
	</match>
</Operation>

