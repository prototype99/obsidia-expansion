<?xml version="1.0" encoding="utf-8" ?>

<Patch>
<!--=============== Vanilla Races Expanded - Saurid ====================-->
<Operation Class="PatchOperationFindMod">
	<mods>
	  <li>Vanilla Races Expanded - Saurid</li>
	</mods>
	<match Class="PatchOperationSequence">
	  <operations>
	    <li Class="PatchOperationAdd">
		  <xpath>/Defs/FactionDef[defName="ObsidiaEmpire" or defName="ObsidiaSyndicateClan" or defName="ObsidiaRenegadeClan" or defName="ObsidiaPilgrimClan" or defName="ObsidiaEmpireSins"]/xenotypeSet/xenotypeChances</xpath>
		  <value>
			<VRESaurids_Saurid MayRequire="Ludeon.RimWorld.Biotech">0.01</VRESaurids_Saurid>
		  </value>
	    </li>
		<li Class="PatchOperationAdd">
		  <xpath>/Defs/FactionDef[defName="ObsidiaCrimsonArachnaCult"]/xenotypeSet/xenotypeChances</xpath>
		  <value>
			<VRESaurids_Saurid MayRequire="Ludeon.RimWorld.Biotech">0.03</VRESaurids_Saurid>
		  </value>
	    </li>
	  </operations>
	</match>
</Operation>

