<?xml version="1.0" encoding="utf-8" ?>

<Defs>
<!--=============== OTC - Pistol ====================-->
  <RecipeDef>
    <defName>OE_Reset_OTC_Pistol_P_Fire</defName>
    <label>Reset Paragon Celestial Prototype(Fire)</label>
    <description>Reset an Paragon Celestial Prototype(Fire).\n\nWarning! Item Quality and Paragon traits will be changed.</description>
    <jobString>Resetting a Paragon Celestial Prototype(Fire).</jobString>
	<workAmount>500</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	<unfinishedThingDef>OE_UnfinishedGear</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>OTC_Pistol_P_Fire</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Syntherius</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>OTC_Pistol_P_Fire</li>
		<li>OE_Syntherius</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <OTC_Pistol_P_Fire>1</OTC_Pistol_P_Fire>
    </products>
	<recipeUsers>
      <li>OE_PersonaTable</li>
    </recipeUsers>
	<skillRequirements>
	  <Crafting>12</Crafting>
	  <Intellectual>12</Intellectual>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
	<researchPrerequisite>OTCGearMod</researchPrerequisite>
  </RecipeDef>

