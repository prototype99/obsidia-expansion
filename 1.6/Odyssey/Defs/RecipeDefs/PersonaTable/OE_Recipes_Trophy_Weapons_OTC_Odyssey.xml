<!--=============== OTC - Cards ====================-->
  <RecipeDef>
    <defName>OE_Bond_OTC_Trophy_Cards</defName>
    <label>Paragon bond Randy's Deck</label>
    <description>Paragon bond a Randy's Deck.</description>
    <jobString>Paragon bonding a Randy's Deck.</jobString>
	<workAmount>1000</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	<unfinishedThingDef>OE_UnfinishedGear</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>OTC_Trophy_Cards</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_PersonaKit</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>OTC_Trophy_Cards</li>
		<li>OE_PersonaKit</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <OTC_Trophy_Cards_P>1</OTC_Trophy_Cards_P>
    </products>
	<recipeUsers>
      <li>OE_PersonaTable</li>
    </recipeUsers>
	<skillRequirements>
	  <Crafting>12</Crafting>
	  <Intellectual>12</Intellectual>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

