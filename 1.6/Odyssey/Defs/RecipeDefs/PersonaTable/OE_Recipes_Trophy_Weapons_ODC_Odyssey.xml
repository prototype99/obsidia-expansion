      <thingDefs>
        <li>ODC_Trophy_BladeLauncher_A</li>
		<li>OE_PersonaKit</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <ODC_Trophy_BladeLauncher_A_P>1</ODC_Trophy_BladeLauncher_A_P>
    </products>
	<recipeUsers>
      <li>OE_PersonaTable</li>
    </recipeUsers>
	<skillRequirements>
	  <Crafting>12</Crafting>
	  <Intellectual>12</Intellectual>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

