  <RecipeDef>
    <defName>OE_Bond_ODC_Trophy_BladeLauncher_A</defName>
    <label>Paragon bond Harvest</label>
    <description>Paragon bond a Harvest.</description>
    <jobString>Paragon bonding a Harvest.</jobString>
	<workAmount>1000</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	<unfinishedThingDef>OE_UnfinishedGear</unfinishedThingDef>
