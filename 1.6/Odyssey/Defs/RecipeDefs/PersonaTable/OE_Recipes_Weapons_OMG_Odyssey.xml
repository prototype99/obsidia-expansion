<?xml version="1.0" encoding="utf-8" ?>

<Defs>
<!--=============== OMG - Pistol ====================-->
  <RecipeDef>
    <defName>OE_Reset_OMG_Pistol</defName>
    <label>Reset Nanofire</label>
    <description>Reset a Nanofire.\n\nWarning! Paragon traits will be changed.</description>
    <jobString>Resetting a Nanofire.</jobString>
	<workAmount>650</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	<unfinishedThingDef>OE_UnfinishedGear</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>OMG_Pistol</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Syntherius</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>OMG_Pistol</li>
		<li>OE_Syntherius</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <OMG_Pistol>1</OMG_Pistol>
    </products>
	<recipeUsers>
      <li>OE_PersonaTable</li>
    </recipeUsers>
	<skillRequirements>
	  <Crafting>12</Crafting>
	  <Intellectual>12</Intellectual>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

