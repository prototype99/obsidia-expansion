<?xml version="1.0" encoding="utf-8" ?>

<Defs>
<!--=============== EGO - Pistol ====================-->
  <RecipeDef>
    <defName>OE_Reset_EGO_Pistol</defName>
    <label>Reset Humunculus</label>
    <description>Reset a Humunculus.\n\nWarning! Paragon traits will be changed.</description>
    <jobString>Resetting a Humunculus.</jobString>
	<workAmount>500</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>EGO_Pistol</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Syntherius</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>EGO_Pistol</li>
		<li>OE_Syntherius</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <EGO_Pistol>1</EGO_Pistol>
    </products>
	<recipeUsers>
      <li>OE_PersonaTable</li>
    </recipeUsers>
	<skillRequirements>
	  <Crafting>12</Crafting>
	  <Intellectual>12</Intellectual>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
	<researchPrerequisite>OEBioengineering</researchPrerequisite>
  </RecipeDef>

