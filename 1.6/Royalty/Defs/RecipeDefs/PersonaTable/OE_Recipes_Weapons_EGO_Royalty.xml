<?xml version="1.0" encoding="utf-8" ?>

<Defs>
<!--=============== EGO - Knife ====================-->
  <RecipeDef>
    <defName>OE_Reset_EGO_Knife</defName>
    <label>Reset Lymocyst</label>
    <description>Reset a Lymocyst.\n\nWarning! Persona traits will be changed.</description>
    <jobString>Resetting a Lymocyst.</jobString>
	<workAmount>500</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>EGO_Knife</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Syntherius</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>EGO_Knife</li>
		<li>OE_Syntherius</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <EGO_Knife>1</EGO_Knife>
    </products>
	<recipeUsers>
      <li>OE_PersonaTable</li>
    </recipeUsers>
	<skillRequirements>
	  <Crafting>12</Crafting>
	  <Intellectual>12</Intellectual>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
	<researchPrerequisite>OEBioengineering</researchPrerequisite>
  </RecipeDef>

