<Defs>
<!--=============== OES - Sword ====================-->
  <RecipeDef>
    <defName>OE_Reset_OES_Sword</defName>
    <label>Reset Heroic Delimiter</label>
    <description>Reset a Heroic Delimiter.\n\nWarning! Persona traits will be changed.</description>
    <jobString>Resetting a Heroic Delimiter.</jobString>
	<workAmount>650</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	<unfinishedThingDef>OE_UnfinishedGear</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>OES_Sword</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Syntherius</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>OES_Sword</li>
		<li>OE_Syntherius</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <OES_Sword>1</OES_Sword>
    </products>
	<recipeUsers>
      <li>OE_PersonaTable</li>
    </recipeUsers>
	<skillRequirements>
	  <Crafting>12</Crafting>
	  <Intellectual>12</Intellectual>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

