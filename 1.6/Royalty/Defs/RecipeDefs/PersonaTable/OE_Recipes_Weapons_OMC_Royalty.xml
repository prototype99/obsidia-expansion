<?xml version="1.0" encoding="utf-8" ?>

<Defs>
<!--=============== OMC - Sword ====================-->
  <RecipeDef>
    <defName>OE_Reset_OMC_Sword_P</defName>
    <label>Reset Persona Delimiter MK-II</label>
    <description>Reset an Persona Delimiter MK-II.\n\nWarning! Item Quality and Persona traits will be changed.</description>
    <jobString>Resetting a Persona Delimiter MK-II.</jobString>
	<workAmount>500</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	<unfinishedThingDef>OE_UnfinishedGear</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>OMC_Sword_P</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Syntherius</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>OMC_Sword_P</li>
		<li>OE_Syntherius</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <OMC_Sword_P>1</OMC_Sword_P>
    </products>
	<recipeUsers>
      <li>OE_PersonaTable</li>
    </recipeUsers>
	<skillRequirements>
	  <Crafting>12</Crafting>
	  <Intellectual>12</Intellectual>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
	<researchPrerequisite>OMCGearMod</researchPrerequisite>
  </RecipeDef>

