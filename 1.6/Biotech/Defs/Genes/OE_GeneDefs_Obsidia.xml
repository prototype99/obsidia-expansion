<?xml version="1.0" encoding="utf-8" ?>

<Defs>
<!--=============== Acid ====================-->
<!--=============== Resistant ====================-->
  <GeneDef>
    <defName>OE_AcidResistant</defName>
    <label>acid resistant</label> 
    <labelShortAdj>acidproof</labelShortAdj> 
    <description>Carriers of this gene are resistant to acid damage from any source. They only take 25% of the acid damage.</description>
    <iconPath>UI/Icons/Genes/OE_AcidResistant</iconPath>
    <displayCategory>ResistanceAndWeakness</displayCategory>
    <displayOrderInCategory>1700</displayOrderInCategory>
    <damageFactors>
      <OTCAcid>0.25</OTCAcid>
    </damageFactors>
    <biostatMet>-2</biostatMet>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>antitox</symbol></li>
		<li><symbol>rough</symbol></li>
      </prefixSymbols>
    </symbolPack>
    <exclusionTags>
      <li>AcidDamage</li>
    </exclusionTags>
  </GeneDef>
<!--=============== Weakness ====================-->
  <GeneDef>
    <defName>OE_AcidWeakness</defName>
    <label>acid weakness</label>
    <labelShortAdj>sensitiveskin</labelShortAdj>
    <description>Carriers of this gene have biological compounds in their skin that react painfully to acid damage. They are unusually sensitive to acid. Damage from acid is multiplied by 4.</description>
    <iconPath>UI/Icons/Genes/OE_AcidWeakness</iconPath>
    <displayCategory>ResistanceAndWeakness</displayCategory>
    <displayOrderInCategory>1701</displayOrderInCategory>
    <damageFactors>
      <OTCAcid>4</OTCAcid>
    </damageFactors>
    <biostatMet>2</biostatMet>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>sensitive</symbol></li>
        <li><symbol>thinskin</symbol></li>
      </prefixSymbols>
    </symbolPack>
    <exclusionTags>
      <li>AcidDamage</li>
    </exclusionTags>
  </GeneDef>

<!--=============== Cryo ====================-->
<!--=============== Resistant ====================-->
