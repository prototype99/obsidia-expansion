<!--=============== Head ====================-->
<!--=============== Cyberware - Ranger ====================-->
  <HediffDef ParentName="OECyberwareLvlParts">
    <defName>OE_CyberwareHead_Ranged</defName>
    <label>cyberware - Ranger</label>
    <labelNoun>a cyberware - Ranger</labelNoun>
    <description>An installed cyberware - Ranger.\n\nThis cyberware can be installed up to 3 times to increase its effect.</description>
    <descriptionHyperlinks><ThingDef>OE_CyberwareHead_Ranged</ThingDef></descriptionHyperlinks>
    <spawnThingOnRemoved>OE_CyberwareHead_Ranged</spawnThingOnRemoved>
    <minSeverity>0</minSeverity>
    <maxSeverity>3</maxSeverity>
	<stages>
	  <li>
        <minSeverity>1</minSeverity>
        <statOffsets>
		  <AimingDelayFactor>-0.05</AimingDelayFactor>
		  <AccuracyTouch>0.05</AccuracyTouch>
		  <AccuracyShort>0.05</AccuracyShort>
		  <AccuracyMedium>0.05</AccuracyMedium>
		  <AccuracyLong>0.05</AccuracyLong>
	      <ShootingAccuracyPawn>2.0</ShootingAccuracyPawn>
	    </statOffsets>
      </li>
      <li>
        <minSeverity>2</minSeverity>
		<statOffsets>
		  <AimingDelayFactor>-0.10</AimingDelayFactor>
		  <AccuracyTouch>0.10</AccuracyTouch>
		  <AccuracyShort>0.10</AccuracyShort>
		  <AccuracyMedium>0.10</AccuracyMedium>
		  <AccuracyLong>0.10</AccuracyLong>
	      <ShootingAccuracyPawn>4.0</ShootingAccuracyPawn>
	    </statOffsets>
      </li>
	  <li>
        <minSeverity>3</minSeverity>
		<statOffsets>
		  <AimingDelayFactor>-0.15</AimingDelayFactor>
		  <AccuracyTouch>0.15</AccuracyTouch>
		  <AccuracyShort>0.15</AccuracyShort>
		  <AccuracyMedium>0.15</AccuracyMedium>
		  <AccuracyLong>0.15</AccuracyLong>
	      <ShootingAccuracyPawn>6.0</ShootingAccuracyPawn>
	    </statOffsets>
      </li>
	</stages>
  </HediffDef>

  <ThingDef ParentName="OEBodyPartsCW">
    <defName>OE_CyberwareHead_Ranged</defName>
    <label>cyberware - Ranger</label>
    <description>Obsidia cyberware is technology installed into the human flesh that sometimes has the capability to interface with the nervous system. Its goal is to achieve higher performance and bring new enhancements in ranged combat.\n\nInstallation requires - cyberware interface.\n\nThis cyberware can be installed up to 3 times to increase its effect.</description>
    <descriptionHyperlinks><ThingDef>OE_ImplantTorso_Interface</ThingDef><HediffDef>OE_CyberwareHead_Ranged</HediffDef></descriptionHyperlinks>
	<graphicData>
      <texPath>Things/Item/Health/Сyberware/Head/OE_Ranger</texPath>
    </graphicData>
    <costList>
      <OE_Mythril>10</OE_Mythril>
	  <OE_FungalWoodLog>10</OE_FungalWoodLog>
      <OE_Syntherius>35</OE_Syntherius>
    </costList>
	<butcherProducts>
	  <OE_Mythril>5</OE_Mythril>
	  <OE_FungalWoodLog>10</OE_FungalWoodLog>
      <OE_Syntherius>15</OE_Syntherius>
	</butcherProducts>
	<comps>
      <li Class="CompProperties_Usable">
        <compClass>CompUsableImplant</compClass>
        <useJob>UseItem</useJob>
        <useLabel>Implant {0_label}</useLabel>
		<userMustHaveHediff>OE_ImplantTorso_Interface</userMustHaveHediff>
      </li>
      <li Class="CompProperties_UseEffectInstallImplant">
        <hediffDef>OE_CyberwareHead_Ranged</hediffDef>
        <bodyPart>Head</bodyPart>
        <canUpgrade>true</canUpgrade>
      </li>
    </comps>
	<recipeMaker>
      <researchPrerequisite>OECyberwares</researchPrerequisite>
    </recipeMaker>
  </ThingDef>
