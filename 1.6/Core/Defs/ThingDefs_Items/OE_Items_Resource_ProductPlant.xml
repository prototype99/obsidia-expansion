      </categories>
      <commonality>1.00</commonality>
      <color>(49,52,73)</color>
      <constructEffect>ConstructWood</constructEffect>
      <appearance>Planks</appearance>
      <soundImpactBullet>BulletImpact_Wood</soundImpactBullet>
      <soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
      <soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
	  <statOffsets>
        <Beauty>30</Beauty>
      </statOffsets>
      <statFactors>
        <MaxHitPoints>2</MaxHitPoints>
        <Beauty>20</Beauty>
        <Flammability>0.5</Flammability>
        <WorkToMake>1.25</WorkToMake>
        <WorkToBuild>1.25</WorkToBuild>
        <DoorOpenSpeed>1.35</DoorOpenSpeed>
		<BedRestEffectiveness>1.35</BedRestEffectiveness>
		<MeleeWeapon_CooldownMultiplier>1.20</MeleeWeapon_CooldownMultiplier>
      </statFactors>
	  <statFactorsQuality>
        <li>
          <stat>PsychicSensitivityFactor</stat>
          <awful>1.10</awful>
          <poor>1.15</poor>
          <normal>1.20</normal>
          <good>1.25</good>
          <excellent>1.30</excellent>
          <masterwork>1.35</masterwork>
          <legendary>1.40</legendary>
        </li>
      </statFactorsQuality>
      <statOffsetsQuality>
        <li MayRequireAnyOf="Ludeon.RimWorld.Royalty,Ludeon.RimWorld.Biotech">
          <stat>PsychicEntropyMaxOffset</stat>
          <awful>10</awful>
          <poor>12</poor>
          <normal>15</normal>
          <good>17</good>
          <excellent>19</excellent>
          <masterwork>22</masterwork>
          <legendary>25</legendary>
        </li>
        <li MayRequireAnyOf="Ludeon.RimWorld.Royalty,Ludeon.RimWorld.Biotech">
          <stat>PsychicEntropyRecoveryRateOffset</stat>
          <awful>0.066</awful>
          <poor>0.066</poor>
          <normal>0.066</normal>
          <good>0.083</good>
          <excellent>0.083</excellent>
          <masterwork>0.083</masterwork>
          <legendary>0.091</legendary>
        </li>
      </statOffsetsQuality>
      <canSuggestUseDefaultStuff>false</canSuggestUseDefaultStuff>
    </stuffProps>
    <isTechHediff>true</isTechHediff>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
	<tradeTags>
      <li>ExoticMisc</li>
	  <li>OEResourcesL</li>
	  <li>OEResourcesSell</li>
    </tradeTags>
	<thingSetMakerTags>
	  <li>RewardStandardMidFreq</li>
	</thingSetMakerTags>
    <techHediffsTags>
      <li>Poor</li>
    </techHediffsTags>
    <weaponClasses>
      <li>Melee</li>
      <li>MeleeBlunt</li>
      <li>Neolithic</li>
    </weaponClasses>
    <tools>
      <li>
        <label>log</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>25</power>
        <cooldownTime>3.0</cooldownTime>
      </li>
    </tools>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
  </ThingDef>

