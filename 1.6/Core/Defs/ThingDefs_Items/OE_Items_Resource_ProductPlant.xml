<!--=============== Fungal Wood ====================-->
  <ThingDef ParentName="ResourceVerbBase">
    <defName>OE_FungalWoodLog</defName>
    <label>fungal wood</label>
    <description>Unique wood. More durable and less flammable, while being lighter.</description>
	<descriptionHyperlinks>
	  <ThingDef>Plant_OE_TreeFungi</ThingDef>
    </descriptionHyperlinks>
    <graphicData>
      <texPath>Things/Item/Resource/ProductPlant/FungalWoodLog</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <equippedAngleOffset>-20</equippedAngleOffset>
    <soundInteract>Wood_Drop</soundInteract>
    <soundDrop>Wood_Drop</soundDrop>
    <statBases>
      <MarketValue>1.5</MarketValue>
      <MaxHitPoints>100</MaxHitPoints>
      <Mass>0.1</Mass> 
      <Flammability>0.05</Flammability>
      <DeteriorationRate>0.65</DeteriorationRate>
      <StuffPower_Armor_Sharp>0.75</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>0.40</StuffPower_Armor_Blunt>
      <StuffPower_Armor_Heat>0.40</StuffPower_Armor_Heat>
      <StuffPower_Insulation_Cold>7</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>7</StuffPower_Insulation_Heat>
      <SharpDamageMultiplier>0.9</SharpDamageMultiplier>
      <BluntDamageMultiplier>0.9</BluntDamageMultiplier>
    </statBases>
    <burnableByRecipe>false</burnableByRecipe>
    <healthAffectsPrice>false</healthAffectsPrice>
    <minRewardCount>10</minRewardCount>
    <stuffProps>
      <stuffAdjective>fungal wooden</stuffAdjective>
      <categories>
        <li>Woody</li>
      </categories>
      <commonality>1.00</commonality>
      <color>(190,190,190)</color>
      <constructEffect>ConstructWood</constructEffect>
      <appearance>Planks</appearance>
      <soundImpactBullet>BulletImpact_Wood</soundImpactBullet>
      <soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
      <soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
      <statFactors>
        <MaxHitPoints>0.75</MaxHitPoints>
        <Beauty>1</Beauty>
        <Flammability>0.05</Flammability>
        <WorkToMake>0.75</WorkToMake>
        <WorkToBuild>0.75</WorkToBuild>
		<MeleeWeapon_CooldownMultiplier>0.75</MeleeWeapon_CooldownMultiplier>
      </statFactors>
      <canSuggestUseDefaultStuff>false</canSuggestUseDefaultStuff>
    </stuffProps>
    <isTechHediff>true</isTechHediff>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
	<tradeTags>
      <li>ExoticMisc</li>
	  <li>OEResourcesL</li>
	  <li>OEResourcesSell</li>
    </tradeTags>
	<thingSetMakerTags>
	  <li>RewardStandardMidFreq</li>
	</thingSetMakerTags>
    <techHediffsTags>
      <li>Poor</li>
    </techHediffsTags>
    <weaponClasses>
      <li>Melee</li>
      <li>MeleeBlunt</li>
      <li>Neolithic</li>
    </weaponClasses>
    <tools>
      <li>
        <label>log</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.6</cooldownTime>
      </li>
    </tools>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
  </ThingDef>

<!--=============== Dragonian Wood ====================-->
  <ThingDef ParentName="ResourceVerbBase">
    <defName>OE_DragonianWoodLog</defName>
    <label>dragonian wood</label>
    <description>Rare type of wood. Very valuable, durable and beautiful, yet heavy.</description>
