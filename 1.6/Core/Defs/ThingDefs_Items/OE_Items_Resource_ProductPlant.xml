	  <ThingDef>Plant_OE_TreeDragonian</ThingDef>
    </descriptionHyperlinks>
    <graphicData>
      <texPath>Things/Item/Resource/ProductPlant/DragonianWoodLog</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <equippedAngleOffset>-20</equippedAngleOffset>
    <soundInteract>Wood_Drop</soundInteract>
    <soundDrop>Wood_Drop</soundDrop>
    <statBases>
      <MarketValue>8.5</MarketValue>
      <MaxHitPoints>200</MaxHitPoints>
      <Mass>0.35</Mass> 
      <Flammability>0.5</Flammability>
      <DeteriorationRate>0.65</DeteriorationRate>
      <StuffPower_Armor_Sharp>0.90</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>0.50</StuffPower_Armor_Blunt>
      <StuffPower_Armor_Heat>0.50</StuffPower_Armor_Heat>
      <StuffPower_Insulation_Cold>4</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>4</StuffPower_Insulation_Heat>
      <SharpDamageMultiplier>1.2</SharpDamageMultiplier>
      <BluntDamageMultiplier>1.2</BluntDamageMultiplier>
    </statBases>
    <burnableByRecipe>false</burnableByRecipe>
    <healthAffectsPrice>false</healthAffectsPrice>
    <minRewardCount>10</minRewardCount>
    <stuffProps>
