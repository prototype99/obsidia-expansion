      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(0.6, 0.6)</drawSize>
    </graphicData>
    <stackLimit>5</stackLimit>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <Mass>0.5</Mass>
      <Beauty>0</Beauty>
      <MarketValue>525</MarketValue>
