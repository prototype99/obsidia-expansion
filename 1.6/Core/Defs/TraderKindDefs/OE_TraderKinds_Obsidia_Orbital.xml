<!--=============== Obsidia Empire - Orbital Common ====================-->
  <TraderKindDef ParentName="OETradersBase">
    <defName>Orbital_Obsidia_Common</defName>
	<label>orbital obsidia goods trader</label>
	<orbital>true</orbital>
    <stockGenerators>
	  <!-- Obs.Meds -->
	  <li Class="StockGenerator_Tag">
        <tradeTag>OEBionicsExotic</tradeTag>
		<thingDefCountRange>2~4</thingDefCountRange>
        <countRange>1~2</countRange>
      </li>
	  
	  <!-- Obs.Gear -->
	  <li Class="StockGenerator_Tag">
        <tradeTag>OMCGear</tradeTag>
		<thingDefCountRange>2~5</thingDefCountRange>
        <countRange>0~2</countRange>
      </li>
	  <li Class="StockGenerator_Tag">
        <tradeTag>OTCGear</tradeTag>
		<thingDefCountRange>2~5</thingDefCountRange>
        <countRange>0~2</countRange>
      </li>
	  <li Class="StockGenerator_Tag">
        <tradeTag>OCCGear</tradeTag>
		<thingDefCountRange>2~5</thingDefCountRange>
        <countRange>0~2</countRange>
      </li>
	  <li MayRequire="Ludeon.RimWorld.Royalty" Class="StockGenerator_Tag">
        <tradeTag>OECGear</tradeTag>
		<thingDefCountRange>2~5</thingDefCountRange>
        <countRange>0~2</countRange>
      </li>
	  <li Class="StockGenerator_Tag">
        <tradeTag>ODCGear</tradeTag>
		<thingDefCountRange>1~3</thingDefCountRange>
        <countRange>0~2</countRange>
      </li>
	  <li Class="StockGenerator_Tag">
        <tradeTag>EGOGear</tradeTag>
		<thingDefCountRange>2~3</thingDefCountRange>
        <countRange>0~2</countRange>
      </li>
	  <li Class="StockGenerator_Tag">
        <tradeTag>CACGear</tradeTag>
		<thingDefCountRange>0~3</thingDefCountRange>
        <countRange>0~2</countRange>
      </li>
	  <li Class="StockGenerator_Tag">
        <tradeTag>OESGear</tradeTag>
		<thingDefCountRange>0~3</thingDefCountRange>
        <countRange>0~2</countRange>
      </li>
	  <li Class="StockGenerator_Tag">
        <tradeTag>OMGGear</tradeTag>
		<thingDefCountRange>0~3</thingDefCountRange>
        <countRange>0~2</countRange>
      </li>
	  <li Class="StockGenerator_Tag">
        <tradeTag>OEGearTrophy</tradeTag>
		<thingDefCountRange>0~3</thingDefCountRange>
        <countRange>0~2</countRange>
      </li>

	  <!-- Books -->
	  <li Class="StockGenerator_SingleDef">
        <thingDef>OE_Datapad</thingDef>
        <countRange>1~3</countRange>
      </li>
	  <li Class="StockGenerator_SingleDef">
        <thingDef>TextBook</thingDef>
        <countRange>0~2</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Novel</thingDef>
        <countRange>0~1</countRange>
      </li>
	  <li Class="StockGenerator_SingleDef">
        <thingDef>Schematic</thingDef>
        <countRange>0~1</countRange>
      </li>
	  <li MayRequire="Ludeon.RimWorld.Anomaly" Class="StockGenerator_SingleDef">
        <thingDef>Tome</thingDef>
        <countRange>0~1</countRange>
      </li>
	  <li MayRequire="DimonSever000.FactionLanguages" Class="StockGenerator_SingleDef">
        <thingDef>LanguageBook</thingDef>
        <countRange>1~3</countRange>
      </li>
	  
	  <!-- Plants -->
      <li MayRequire="Ludeon.RimWorld.Biotech" Class="StockGenerator_SingleDef">
        <thingDef>PoluxSeed</thingDef>
        <countRange>0~1</countRange>
      </li>
	  <li Class="StockGenerator_Tag">
        <tradeTag>EGOSeed</tradeTag>
        <thingDefCountRange>0~1</thingDefCountRange>
      </li>
	  
	  <!-- Obs.Buildings -->
	  <li Class="StockGenerator_Tag">
        <tradeTag>OEBuilding</tradeTag>
		<thingDefCountRange>2~6</thingDefCountRange>
        <countRange>1~2</countRange>
      </li>
	  <li Class="StockGenerator_Tag">
        <tradeTag>OEBuildingBeacons</tradeTag>
		<thingDefCountRange>2~3</thingDefCountRange>
        <countRange>2~3</countRange>
      </li>
	  <li Class="StockGenerator_Tag">
        <tradeTag>OEBuildingExotic</tradeTag>
		<thingDefCountRange>-1~3</thingDefCountRange>
        <countRange>1~1</countRange>
      </li>
	  
	  <!-- Slaves -->
	  <li Class="StockGenerator_Slaves">
        <respectPopulationIntent>true</respectPopulationIntent>
        <countRange>2~8</countRange>
      </li>
	  
      <!-- Animals -->
      <li Class="StockGenerator_Animals">
        <checkTemperature>false</checkTemperature>
		<maxWildness>1.00</maxWildness>
        <tradeTagsSell>
          <li>AnimalExotic</li>
		  <li>AnimalUncommon</li>
		  <li>AnimalFarm</li>
          <li>AnimalPet</li>
        </tradeTagsSell>
        <kindCountRange>2~4</kindCountRange>
        <countRange>2~4</countRange>
        <createMatingPair>
          <li>AnimalFarm</li>
        </createMatingPair>
      </li>
	  <li Class="StockGenerator_Animals">
        <checkTemperature>false</checkTemperature>
		<maxWildness>1.00</maxWildness>
        <tradeTagsSell>
          <li>OEAnimal</li>
          <li>OEAnimalSlime</li>
        </tradeTagsSell>
        <tradeTagsBuy>
          <li>OEAnimalExotic</li>
		  <li>OEAnimalSell</li>
        </tradeTagsBuy>
        <kindCountRange>2~4</kindCountRange>
        <countRange>2~4</countRange>
        <createMatingPair>
          <li>OEAnimal</li>
        </createMatingPair>
      </li>	
	  
	  <!-- Exotic -->
	  <li Class="StockGenerator_Tag">
        <tradeTag>Artifact</tradeTag>
        <thingDefCountRange>1~4</thingDefCountRange>
        <countRange>1~1</countRange>
      </li>
	  <li MayRequire="Ludeon.RimWorld.Biotech" Class="StockGenerator_SingleDef">
        <thingDef>Genepack</thingDef>
        <countRange>5~8</countRange>
      </li>
	  <li MayRequire="Ludeon.RimWorld.Biotech" Class="StockGenerator_SingleDef">
        <thingDef>ArchiteCapsule</thingDef>
        <countRange>0~1</countRange>
      </li>
	  <li MayRequire="sumghai.Medpod" Class="StockGenerator_SingleDef">
        <thingDef>IsolinearProcessor</thingDef>
        <countRange>-1~2</countRange>
      </li>
	  <li MayRequire="sumghai.Replimat" Class="StockGenerator_SingleDef">
        <thingDef>IsolinearModule</thingDef>
        <countRange>-2~1</countRange>
      </li>
	  <li MayRequire="GoGaTio.MechanoidUpgrades" Class="StockGenerator_Tag">
        <tradeTag>MU_Upgrade_TraderSells</tradeTag>
		<thingDefCountRange>4~6</thingDefCountRange>
        <countRange>4~8</countRange>
      </li>
	  <li Class="StockGenerator_Tag">
        <tradeTag>ExoticMisc</tradeTag>
        <thingDefCountRange>3~5</thingDefCountRange>
        <countRange>2~3</countRange>
        <excludedThingDefs>
          <li>Luciferium</li>
          <li>ComponentSpacer</li>
        </excludedThingDefs>
        <customCountRanges>
          <MedicineUltratech>2~15</MedicineUltratech>
          <Hyperweave>50~200</Hyperweave>
        </customCountRanges>
      </li>
	  <li Class="StockGenerator_SingleDef" MayRequire="Ludeon.RimWorld.Odyssey">
        <thingDef>GravlitePanel</thingDef>
        <countRange>-10~100</countRange>
      </li>
	  <li Class="StockGenerator_SingleDef" MayRequire="Ludeon.RimWorld.Odyssey">
        <thingDef>ShuttleEngine</thingDef>
        <countRange>0~1</countRange>
      </li>
    </stockGenerators>
  </TraderKindDef>

