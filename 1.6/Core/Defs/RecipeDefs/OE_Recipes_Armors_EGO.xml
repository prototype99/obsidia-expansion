<!--=============== Suit ====================-->
  <RecipeDef>
    <defName>OE_Gestate_EGO_Armor_Suit</defName>
    <label>gestate EGO Suit</label>
    <description>Gestate an amalgam EGO Suit.</description>
    <jobString>Gestating an EGO Suit.</jobString>
	<workAmount>30000</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
	<unfinishedThingDef>OE_UnfinishedAmalgama</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>MeatRaw</li>
          </categories>
        </filter>
        <count>100</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Platinum</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Mooncloth</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Syntherius</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_EGOAmalgamationCore</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
	<fixedIngredientFilter>
	  <categories>
        <li>MeatRaw</li>
      </categories>  
	  <thingDefs>
		<li>OE_Platinum</li>
		<li>OE_Mooncloth</li>
		<li>OE_Syntherius</li>
		<li>OE_EGOAmalgamationCore</li>
      </thingDefs>
	  <disallowedThingDefs>
        <li>OE_Pearlapple_Fruit</li>
		<li>OE_Red_TionantJelly</li>
        <li>InsectJelly</li>
      </disallowedThingDefs>
    </fixedIngredientFilter>
    <products>
      <EGO_Armor_Suit>1</EGO_Armor_Suit>
    </products>
	<recipeUsers>
      <li>OE_AmalgamVat</li>
    </recipeUsers>
	<skillRequirements>
	  <Crafting>6</Crafting>
	  <Intellectual>6</Intellectual>
	  <Medicine>6</Medicine>
    </skillRequirements>
    <workSkill>Intellectual</workSkill>
	<researchPrerequisites>
	  <li>OEGearCross</li>
	  <li>OEBioengineering</li>
	</researchPrerequisites>
  </RecipeDef>

