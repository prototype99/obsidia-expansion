<!--=============== Elder Tionant Base ====================-->
  <ThingDef Name="OEBaseTionantElder" ParentName="AnimalThingBase" Abstract="True">
    <devNote>obsidia</devNote>
    <statBases>
      <MoveSpeed>2.0</MoveSpeed>
      <MarketValue>8000</MarketValue>
      <ComfyTemperatureMin>-75</ComfyTemperatureMin>
	  <ComfyTemperatureMax>75</ComfyTemperatureMax>
	  <ArmorRating_Sharp>1.1</ArmorRating_Sharp>
	  <ArmorRating_Blunt>1.0</ArmorRating_Blunt>
	  <ArmorRating_Heat>0.8</ArmorRating_Heat>
	  <ToxicResistance>0.95</ToxicResistance>
	  <ToxicEnvironmentResistance MayRequire="Ludeon.RimWorld.Biotech">0.95</ToxicEnvironmentResistance>
      <FilthRate>10</FilthRate>
	  <Wildness>0.99</Wildness>
	  <VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">1</VacuumResistance>
    </statBases>
	<uiIconScale>1.2</uiIconScale>
    <tools>
      <li>
        <label>left claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>12</power>
        <cooldownTime>1.8</cooldownTime>
		<armorPenetration>0.30</armorPenetration>
        <linkedBodyPartsGroup>OELeftInsectClawAttackTool</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
        <chanceFactor>0.5</chanceFactor>
      </li>
      <li>
        <label>right claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>12</power>
        <cooldownTime>1.8</cooldownTime>
		<armorPenetration>0.30</armorPenetration>
        <linkedBodyPartsGroup>OERightInsectClawAttackTool</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
        <chanceFactor>0.5</chanceFactor>
      </li>
      <li>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>15</power>
        <cooldownTime>1.8</cooldownTime>
		<armorPenetration>0.30</armorPenetration>
        <linkedBodyPartsGroup>HeadClaw</linkedBodyPartsGroup>
        <chanceFactor>0.9</chanceFactor>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>OTCAcid</def>
              <amount>15</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>1.8</cooldownTime>
		<armorPenetration>0.30</armorPenetration>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <body>OETionantLike</body>
	  <packAnimal>false</packAnimal>
	  <herdMigrationAllowed>false</herdMigrationAllowed>
      <baseBodySize>0.85</baseBodySize>
      <baseHealthScale>22.5</baseHealthScale>
      <foodType>VegetarianRoughAnimal, DendrovoreAnimal</foodType>
      <leatherDef>OE_Obsidian</leatherDef>
