<!--=============== OEC - Knife ====================-->
  <RecipeDef>
    <defName>OE_Reset_OEC_Knife_P</defName>
    <label>Reset Persona Exalted Shard</label>
    <description>Reset an Persona Exalted Shard.\n\nWarning! Item Quality and Persona traits will be changed.</description>
    <jobString>Resetting a Persona Exalted Shard.</jobString>
	<workAmount>500</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	<unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>OEC_Knife_P</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>OE_Syntherius</li>
          </thingDefs>
