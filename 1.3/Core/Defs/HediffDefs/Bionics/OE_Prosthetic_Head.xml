<?xml version="1.0" encoding="utf-8" ?>

<Defs>
<!--=============== Eyes ====================-->
<!--=============== Prosthetic - Normal ====================-->
  <HediffDef ParentName="OEAddBodyParts">
    <defName>OE_ProstheticEye</defName>
    <label>bionic eye</label>
    <labelNoun>a bionic eye</labelNoun>
    <description>An installed bionic eye.</description>
    <descriptionHyperlinks><ThingDef>OE_ProstheticEye</ThingDef></descriptionHyperlinks>
    <spawnThingOnRemoved>OE_ProstheticEye</spawnThingOnRemoved>
    <woundAnchorRange>0</woundAnchorRange>
    <displayWound>true</displayWound>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>1.35</partEfficiency>
      <betterThanNatural>true</betterThanNatural>
    </addedPartProps>
	<stages>
	  <li>
		<hungerRateFactorOffset>0.05</hungerRateFactorOffset>
	  </li>
	</stages>
  </HediffDef>

  <ThingDef ParentName="OEBodyParts">
    <defName>OE_ProstheticEye</defName>
    <label>bionic eye</label>
    <description>Obsidia bionic eye. It is better than a biological eye in almost every way. Even if it is harmed, it repairs itself over time. The use of this prosthesis puts a small strain on the body.</description>
    <descriptionHyperlinks><RecipeDef>OE_InstallProstheticEye</RecipeDef></descriptionHyperlinks>
	<graphicData>
      <texPath>Things/Item/Health/Prosthetic/OE_Eye</texPath>
    </graphicData>
    <costList>
      <OE_PlatinumSynthweave>35</OE_PlatinumSynthweave>
	  <OE_Obsidian>10</OE_Obsidian>
      <OE_Syntherius>20</OE_Syntherius>
    </costList>
	<butcherProducts>
	  <OE_PlatinumSynthweave>25</OE_PlatinumSynthweave>
	  <OE_Obsidian>5</OE_Obsidian>
      <OE_Syntherius>10</OE_Syntherius>
	</butcherProducts>
    <statBases>
      <Mass>0.5</Mass>
    </statBases>
  </ThingDef>

  <RecipeDef ParentName="OESurgeryInstallBodyPartBase">
    <defName>OE_InstallProstheticEye</defName>
    <label>install bionic eye</label>
    <description>Install a bionic eye.</description>
    <descriptionHyperlinks>
      <ThingDef>OE_ProstheticEye</ThingDef>
      <HediffDef>OE_ProstheticEye</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing bionic eye.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>OE_ProstheticEye</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>OE_ProstheticEye</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Eye</li>
    </appliedOnFixedBodyParts>
    <addsHediff>OE_ProstheticEye</addsHediff>
  </RecipeDef>

  <RecipeDef ParentName="SurgeryRemoveImplantBase">
    <defName>OE_RemoveProstheticEye</defName>
    <label>remove bionic eye</label>
    <description>Remove a bionic eye.</description>
    <jobString>Removing bionic eye.</jobString>
    <removesHediff>OE_ProstheticEye</removesHediff>
  </RecipeDef>
